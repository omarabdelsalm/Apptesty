<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Apptesty.Contactpage">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <Image Source="ae.jpg" Aspect="AspectFill" HeightRequest="400" WidthRequest="100"/>
            <Button Text="SMS" Clicked="Smsbtn" BackgroundColor="Blue" FontSize="30"/>
            <Button Text="Email" Clicked="Emlbtn" BackgroundColor="DarkGray" FontSize="30"/>
            <Button Text="Call Me" Clicked="Callbtn" BackgroundColor="DarkGoldenrod" FontSize="30"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>