<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             BackgroundImageSource="a122.png"
             x:Class="Apptesty.LoginPage">
    <ContentPage.Content>
        <StackLayout  VerticalOptions="End" Margin="20" >
       
            <Entry Placeholder="User New Email"  x:Name="UserNewEmail" BackgroundColor="White" FontSize="Large" TextColor="Black"></Entry>
            <Entry Placeholder="User New Password"  x:Name="UserNewPassword" BackgroundColor="White" FontSize="Large" TextColor="Black"></Entry>
    
            <Button BackgroundColor="Black" TextColor="White" Text="تسجيل حساب جديد" Clicked="signupbutton_Clicked" x:Name="signupbutton"/>
           
        </StackLayout>
    </ContentPage.Content>
</ContentPage>